%table.table
  %thead
    %tr
      %th Number
      %th Completed At
      %th Total
      %th Actions
  %tbody
    - orders.each do |order|
      %tr
        %td= link_to order.number, order
        %td= order.completed_date
        %td= order.calculate_total_price
        %td= link_to "View", order_path(order)